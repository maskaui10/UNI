    "def heat_exact_surfaceplot():\n",
    "    x = t = np.linspace(0, 1, 100, endpoint=True)\n",
    "    xx, tt = np.meshgrid(x, t) #für xx, entsteht eine 50x50 matrix bei der zeilenweise mit x aufgeüllt wird\n",
    "                                #und für yy, entsteht eine 50x50 matrix bei der spaltenweise mit y aufgeüllt wird\n",
    "    z = heat_exact(xx, tt) #nutzen meshgrid nur um für jedes tupel (x_i,y_j) ein funktionswert z_i,j zu erhalten\n",
    "    \n",
    "    fig = plt.figure()\n",
    "    ax = plt.axes(projection='3d')\n",
    "    ax.plot_surface(xx, tt, z, rstride=1, cstride=1, cmap='viridis', edgecolor='none')\n",
    "    ax.set_title('heat_exact')\n",
    "    ax.set_xlabel('x')\n",
    "    ax.set_ylabel('t')\n",
    "    ax.set_zlabel('heat_exact(x,t)')\n",
    "    \n",
    "    plt.show()\n",
    "    \n",
    "    return True\n",
    "\n",
    "\n",
    "\n",
    "heat_exact_surfaceplot()"
